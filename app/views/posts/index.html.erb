
<% if notice %>
<div class="alert alert-success text-center"  role="alert">
  <h4><strong><%= notice %></strong></h4>
  <% end %>
  
</div>
<div class="container">
<h1>Posts</h1>

<div id="posts grid-container">
  <% @posts.each do |post| %>
    <%= render post %>

  <% end %>
</div>


<%= link_to "New post", new_post_path, class:"btn btn-lg btn-success float-end" %>
</div>
